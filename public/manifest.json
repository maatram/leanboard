{
  "manifest_version": 2,
  "short_name": "Lean Board",
  "name": "Lean Board",
  "version": "1.0.0",
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage", "tabs",
    "https://*/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com https://apis.google.com; object-src 'self'; "
}
